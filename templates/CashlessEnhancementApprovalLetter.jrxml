					<paragraph leftIndent="2" rightIndent="2"/>
				</textElement>
				<textFieldExpression><![CDATA["In case of any difficulty in discharge or with Hospital services, call or SMS "+($F{hrmUserName} != null ? $F{hrmUserName} : "")+" ("+($F{hrmContactNumber} != null ? $F{hrmContactNumber} : " ")+"), Region Head : "+($F{hrmHeadName} != null ? $F{hrmHeadName} : " ") +" ("+($F{hrmHeadContactNo} != null ? $F{hrmHeadContactNo} : " ")+")"]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true">
