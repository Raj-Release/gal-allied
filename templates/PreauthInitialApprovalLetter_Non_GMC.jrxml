					<printWhenExpression><![CDATA[($F{proposedPanNumber} == null) &&(($F{amountToHospAftPremium} != null && $F{amountToHospAftPremium}.longValue() >= 100000)  || ( $F{initialTotalApprovedAmt} != null && ($F{initialTotalApprovedAmt}.longValue() + ($F{otherBenfitOpt} != null && $F{otherBenfitOpt} ?($F{totalOtherBenefitsApprovedAmt} != null ? $F{totalOtherBenefitsApprovedAmt}.longValue() : 0) : 0)) >= 100000))]]></printWhenExpression>
